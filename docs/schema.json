"idee-dashboard": {
  // user
  // privilegies: 0 => admin, 1 => employ, 2 => customer
  "users": {
    "UID": { // row
      "privilegies": "number"
    }
  }

  // login
  "projects": {
    "PROJECT_NAME": { // row
      "contact": {
        "ID": {
          "viewed": "boolean"
        }
      }
    }
  },

  // anonymous
  "contact": {
    "PROJECT_NAME": [ // row
      "ID": {
        // contacts
        "date": "date"
        "email": "email"
        "message": "text"
        "name": "string"
        "phone": "number",
        "page": "string",
        "extra": {
          // own properties
        }
      }
    ]
  }
}
